    "hono-rate-limiter": "^0.2.0",
    "sharp": "^0.33.2",
    "stripe": "^14.12.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.8.0",
