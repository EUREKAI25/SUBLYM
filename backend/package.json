{
  "name": "sublym-backend",
  "version": "1.0.0",
  "description": "SUBLYM API Backend - Dream Visualization Platform",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
